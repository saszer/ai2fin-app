{
  "name": "@ai2/connectors",
  "version": "1.1.0",
  "description": "Bank and email connectors for AI2 Enterprise Platform",
  "main": "dist/index.js",
  "scripts": {
    "build": "prisma generate --schema=./prisma/schema.prisma && tsc",
    "start": "node dist/server.js",
    "dev": "tsx src/server.ts",
    "clean": "rimraf dist",
    "test": "jest",
    "lint": "eslint src/**/*.ts --fix",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "postinstall": "node scripts/postinstall.js"
  },
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "@sentry/node": "^8.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "socket.io": "^4.8.1",
    "winston": "^3.19.0",
    "winston-syslog": "^2.7.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "prisma": "^5.7.1",
    "rimraf": "^5.0.5",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3"
  }
}